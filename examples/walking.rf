#Import the definitions library "roboparts"
@import src="robotics/definitions/roboparts.rdef"

<variables>
var = walkForwardTen {
move.forward = (10);
}

var = walkForwardTwenty {
move.forward = (20);
}
</variables>

<robocode mode="async">
repeat = (2) {
move.forward = (10);
turn.left;
move.forward = (5);
turn.right;
move.backward = (30);

}
</robocode>
